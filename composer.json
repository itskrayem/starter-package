{
  "name": "itskrayem/starter-package",
  "description": "Company Laravel Starter Kit",
  "type": "library",
  "license": "proprietary",
  "require": {
    "php": ">=8.1",
    "laravel/nova": "^5.0",
    "tinymce/tinymce": "^7.0",
    "spatie/laravel-medialibrary": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "YourOrg\\StarterKit\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
            "ItsKrayem\\StarterPackage\\StarterPackageServiceProvider"
      ]
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan starter:install-core --ansi"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
