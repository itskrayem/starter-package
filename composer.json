{
  "name": "yourcompany/laravel-app",
  "description": "Laravel application with starter package",
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^8.1",
    "laravel/framework": "^10.0",
    "laravel/nova": "^5.0",
    "tinymce/tinymce": "^7.0",
    "spatie/laravel-medialibrary": "^11.0",
    "itskrayem/starter-package": "dev-main"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/yourusername/starter-package.git"
    },
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump"
    ]
  }
}
