{
  "name": "itskrayem/starter-package",
  "description": "Company Laravel Starter Package",
  "type": "library",
  "license": "proprietary",
  "require": {
    "php": ">=8.1",
    "laravel/framework": "^12.0"
  },
  "autoload": {
    "psr-4": {
      "ItsKrayem\\StarterPackage\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ItsKrayem\\StarterPackage\\StarterPackageServiceProvider"
      ],
      "stubs": [
        "stubs/"
      ]
    }
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/itskrayem/starter-package.git"
    }
  ],
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
