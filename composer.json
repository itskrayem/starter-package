{
  "name": "itskrayem/starter-package",
  "description": "Company Laravel Starter Package",
  "type": "library",
  "license": "proprietary",
  "require": {
    "php": ">=8.1",
    "laravel/nova": "^5.0",
    "tinymce/tinymce": "^7.0",
    "spatie/laravel-medialibrary": "^11.0",
    "spatie/laravel-permission": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "ItsKrayem\\StarterPackage\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ItsKrayem\\StarterPackage\\StarterPackageServiceProvider"
      ]
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    },
    {
      "type": "vcs",
      "url": "git@github.com:itskrayem/starter-package.git"
    }
  ],
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan starter:install-core --ansi"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
